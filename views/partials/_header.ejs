<title>TinyApp</title>
<% if (user) { %>
  <h1>Logged in as <%= user %></h1>
  <form method = 'POST' action = '/logout'>
    <button>Logout</button>
  </form>
<% } else { %>
  <form method = 'GET' action = '/login'>
    <button>Login</button>
  </form>

  <form method = 'GET' action = '/register'>
    <button>Register</button>
  </form>
<% } %>

