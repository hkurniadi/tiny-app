<!DOCTYPE html>
<html lang="en">
<head>
  <% include ./partials/_header %>
</head>
<body>
  <ul>
    <% for (let shortURL in urls) { %>
      <li><%= `${shortURL} - ${urls[shortURL]}` %></li>
      <a href = '/urls/<%= shortURL %>'>Edit URL</a>
      <form method='POST' action='/urls/<%= shortURL %>/delete'>
        <input type='submit' value='Delete URL'>
      </form>
    <% } %>
  </ul>

  <ul>
    <li><a href='/urls/new'>Click here to shorten your URL</a></li>
  </ul>
</body>
</html>